FoamFile
{
    format      ascii;
    class       dictionary;
    location    "system";
    object      fvSchemes;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

d2dt2Schemes
{
    default         none;
    d2dt2(u)        Euler;
}

gradSchemes
{
    default         Gauss linear;
}

divSchemes
{
    default         none;
    //div(c2, u)      Gauss linear;
    div(u)          Gauss linear;
}

laplacianSchemes
{
    default             none;
    laplacian(c2,u)    Gauss linear orthogonal;
}

